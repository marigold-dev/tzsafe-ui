    "@ariakit/react": "^0.1.8",
    "@radix-ui/react-icons": "^1.1.1",
    "@radix-ui/react-select": "^1.2.0",
    "@radix-ui/react-tooltip": "^1.0.3",
    "@taquito/beacon-wallet": "^17.2.0",
    "@taquito/taquito": "^17.2.0",
    "@taquito/tzip16": "^17.2.0",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "@types/uuid": "^9.0.1",
    "@wert-io/widget-initializer": "^4.0.1",
    "assert-never": "^1.2.1",
    "bignumber.js": "^9.1.1",
    "bs58check": "^3",
    "env-cmd": "^10.1.0",
    "eslint": "8.28.0",
    "eslint-config-next": "13.0.5",
    "formik": "2.4.1",
    "fp-ts": "^2.16.1",
    "io-fs": "^1.0.0",
    "io-ts": "^2.2.21",
    "jsdom": "^23.0.1",
    "luxon": "^3.2.1",
    "next": "13.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tiny-emitter": "^2.1.0",
    "typescript": "4.9.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/luxon": "^3.2.0",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.13",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "pino-pretty": "^10.2.3",
    "postcss": "^8.4.19",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.3",
    "tailwindcss": "^3.2.4",
    "vite": "^5.0.10",
    "vitest": "^0.29.8"
  }
